    "version": "0.2.0",
    "configurations": [{
        "name": "启动",
        "type": "lldb",
        "request": "launch",
        "cwd": "${workspaceRoot}",
        "console": "internalConsole",
        "sourceLanguages": ["rust"],
        "cargo": {
            "args": [
                "run",
                "--no-default-features",
                "${input:ambassador-where}",
                "--bin",
                "${fileBasenameNoExtension}"
            ]
        }
    }],
    "inputs": [{
        "type": "pickString",
        "id": "ambassador-where",
        "description": "ambassador where?",
        "options": [
          "--no-default-features",
          "--features=ambassador-where"
        ],
        "default": "--features=ambassador-where"
      }]
}
